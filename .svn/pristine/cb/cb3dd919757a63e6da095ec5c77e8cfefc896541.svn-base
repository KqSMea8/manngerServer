<font color='#027DD1'>1.设置斑马纹、指定列样式：</font>
<strong>js部分:</strong>
 var taxArr = [{ name: '额外税1',value:'10.00'},{name:'额外税2',value:'20.00'},{name:'额外税3',value:'30.00'}] ;
 var stylesGridObj = new biz.grid({
      id:"#styles_rowed",/*html部分table id*/
      data: testData,/*数组数据*/
      datatype: "local",/*数据类型，设置为本地数据*/
      editurl:"http://localhost:8080/edit/" ,/*新增、编辑、删除数据交互地址*/
      colModel:[
           {name:'act',index:'act', label:"Actions",width:75,sortable:false,hidden:true},
           {name:'ids',index:'ids',label:"标识",key:true, width:55,editable:true,editoptions:{readonly:false},
              formoptions:{ rowpos:1, colpos:1},<font color="red">classes:"backcolor1"</font>},
           {name:'invdate',index:'invdate',label:"有效期",key:false,width:180,editable:true,editrules:{date:true},edittype:'datepicker',
              editoptions:{dateFormat:"yy-mm-dd"},formoptions:{ rowpos:1, colpos:2}},
           {name:'name',index:'name', label:"名称",key:false,width:90,editable:false,edittype:"select",
              editoptions:{value:"FE:name1;IN:name2;TN:name3;AR:name4"},<font color="red">classes:"backcolor2"</font>},
           {name:'amount',label:"缴税总量",index:'amount', width:100, align:"right",editrules:{required:true},editable:true,
              formoptions:{ rowpos:2, colpos:1}},
           {name:'tax',label:"缴税",index:'tax', width:80, align:"right",editable:true,edittype:"select",
                     editoptions:{data:taxArr},/*指定select数据源*/
                     formatter:false,/*false：编辑框显示name，true：编辑框显示value*/
                     formoptions:{ rowpos:2, colpos:2}
                     },
           {name:'total',label:"总量",index:'total', width:70,align:"right",editable:true,formoptions:{ rowpos:3, colpos:1}},
           {name:'note',index:'note', label:"备注",width:100, sortable:false,editable:true,edittype:"textarea",
              editoptions:{rows:"2"},formoptions:{ rowpos:3, colpos:2},<font color="red">classes:"backcolor4"</font>}
     ],
     <font color="red">altRows:true</font>/*设置斑马纹样式可用*/,
     <font color="red">altclass:"even"</font>/*设置斑马纹样式为even*/ ,
     navtype:"top" /*导航栏类型*/,
     pager: '#styles_prowed' /*分页栏id*/,
     pView:{
         top:200, /*弹出框top位置*/
         left:250,/*弹出框left位置*/
         width:600,/*宽度*/
         resize:false,
         recreateForm:true
     },
     pAdd:{ 
         top:200, /*弹出框top位置*/
         left:250,/*弹出框left位置*/
         width:750,/*宽度*/
         resize:true,/*弹出框可缩放大小*/
         url:" http://localhost",/*新增数据提交url，如不设置使用editurl*/
         recreateForm:true/*每次新增数据重新创建弹出框*/},/*增加数据弹出框属性*/
     pEdit:{
      	 top:200, /*弹出框top位置*/
         left:250,/*弹出框left位置*/
         width:750,/*宽度*/
      	 resize:true,url:" http://localhost",recreateForm:true}
   });
