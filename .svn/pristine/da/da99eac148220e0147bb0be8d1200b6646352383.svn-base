<strong>js部分:</strong>
    new biz.button({id:"#getmethods_getrow",label: "操作" }).click(function() {
           var id = <font color="red">getmethodsObj.getGridParam('selrow');</font>/*获取选中行序号*/
           var ret = <font color="red">getmethodsObj.getRowData(id);</font>/*获取选中行数据*/
           new biz.alert({type:"alert",message:JSON.stringify(ret),title:"提 示"}) ;
	});
    $( "#getmethods_getCell" ).change(function() {
           var su = <font color="red">getmethodsObj.getCell(1,$( "#getmethods_getCell" ).val());</font>/*获取指定单元格值*/
           new biz.alert({type:"alert",message:su,title:"提 示"}) ;
	});
    $( "#getmethods_getCol" ).change(function() {
           var su = <font color="red">getmethodsObj.getCol("amount",false, $( "#getmethods_getCol" ).val());</font>/*获取指定列值*/
           new biz.alert({type:"alert",message:su,title:"提 示"}) ;
	});
    new biz.button({id:"#getmethods_getDataIDs",label: "操作" }).click(function() {
          var id = <font color="red">getmethodsObj.getDataIDs();</font>/*获取所有数据id值*/
          new biz.alert({type:"alert",message:JSON.stringify(id),title:"提 示"}) ;
	});
    $( "#getmethods_getInd" ).change(function() {
          var id = <font color="red">getmethodsObj.getInd($( "#getmethods_getInd" ).val()); </font>/*获取指定行在当前grid中序号*/
          new biz.alert({type:"alert",message:id,title:"提 示"}) ;
	});
    $( "#getmethods_getGridParam" ).change(function() {
           /*获取grid参数，部分参数不能通过此方法获取，详细参加属性说明文档*/
           var su = <font color="red">getmethodsObj.getGridParam( $( "#getmethods_getGridParam" ).val());</font>
           new biz.alert({type:"alert",message:su,title:"提 示"}) ;
	});
