<script type="text/javascript">

new biz.button({id:"#btn0_button",label: "普通按钮" });
var btndis = new biz.button({id:"#btn1_button",label: "不可用按钮",disabled:true});

 new biz.button({id:"#submit_button_1"}).click(function() {
    alert("提交成功");
 });

 new biz.button({id:"#btn2_button",icons: {primary: "ui-icon-disk"},label: "提交" , text: true}).click(function() {
     alert( "primary代表左图标" );
  });

 new biz.button({id:"#btn3_button",icons: {secondary:'ui-icon-note'},label: "选择" , text: true}).click(function() {
     alert( "secondary代表右图标" );
  });

/*radio button*/
var radioArr = [
    {label: "研发经理", name: "position", value: "researchManager"},
    {label: "产品经理", name: "position", value: "productManager"}
  ];
var checkboxArr = [
   {label: "踢足球", name: "intrest", value: "football"},
   {label: "打篮球", name: "intrest", value: "basketball"},
   {label: "打乒乓球", name: "intrest", value: "pingpong"},
   {label: "跳舞 ", name: "intrest", value: "dance"}
 ];

    new biz.radio({id:"#radio_button",data: radioArr})

/*checkbox button*/
    var obj = new biz.checkbox({id:"#checkbox_button",data:checkboxArr});
    new biz.button({id:"#getUncheckValues"}).click(function() {
        alert(obj.getUncheckValues());
    });
     new biz.button({id:"#getCheckValues"}).click(function() {
        alert(obj.getCheckValues());
    });
    new biz.button({id:"#check"}).click(function() {
        obj.check("input[name='intrest'][value='football']");
    });
     new biz.button({id:"#uncheck"}).click(function() {
        obj.uncheck("input[name='intrest'][value='football']");
    });
    new biz.button({id:"#checkAll"}).click(function() {
        obj.checkAll();
    });
    new biz.button({id:"#uncheckAll"}).click(function() {
         obj.uncheckAll();
    });
    new biz.button({id:"#toggle"}).click(function() {
         obj.toggles();
    });
     new biz.button({id:"#enable"}).click(function() {
        obj.enable("input[name='intrest'][value='football']");
    });
    new biz.button({id:"#disable"}).click(function() {
        obj.disable("input[name='intrest'][value='football']");
    });
    new biz.button({id:"#enableAll"}).click(function() {
         obj.enableAll();
    });
    new biz.button({id:"#disableAll"}).click(function() {
         obj.disableAll();
    });

</script>